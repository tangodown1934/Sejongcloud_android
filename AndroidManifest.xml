            android:name="app.sejongcloud.library.Library"
            android:label="tab5"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="app.sejongcloud.tip.Tip"
            android:label="tab6"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar" >
